<h1>マイページ</h1>

<p>"こんにちは#{current_user.nickname}さん"</p>




  <%= month_calendar events: @dishes do |date,dishes| %>
    <%= date %> 
    <% dishes.each do |dish| %> 
      <p><%= dish.dish_1%></p>
      <p><%= dish.dish_2%></p>
      <p><%= dish.dish_3%></p>
      <p><%= dish.dish_4%></p>
      <p><%= dish.dish_5%></p>
    <% end %>  
  <% end %>